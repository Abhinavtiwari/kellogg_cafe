

<div class="page-header">
  <h1>Menu Items</h1>
</div>


<table class="table table-hover">
  <thead>
    <tr>
      <th>Availability</th>
      <th>Item name</th>
      <th>Item category</th>
      <th>Item picture</th>
      <th>Item price</th>
      <th>Item description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @menu.each do |menu| %>
      <tr>
        <td><%= menu.availability %></td>
        <td><%= menu.item_name %></td>
        <td><%= menu.item_category %></td>
        <td><%= image_tag menu.item_picture, width:80, class: "img-square" %></td>
        <td><%= menu.item_price %></td>
        <td><%= menu.item_description %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

