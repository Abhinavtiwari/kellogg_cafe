<div class="page-header">
  <h1>User #<%= @user.id %> Profile</h1>
</div>

<dl class="dl-horizontal">
  

  <dd><%= image_tag @user.picture, width:200, class: "img-circle" %></dd>

  <dt>First name</dt>
  <dd><%= @user.first_name %></dd>

  <dt>Last name</dt>
  <dd><%= @user.last_name %></dd>

  <dt>Email</dt>
  <dd><%= @user.email %></dd>

  <dt>Password</dt>
  <dd><%= @user.password %></dd>


</dl>
